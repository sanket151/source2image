echo "In the assmeble file"

echo "Installing required packages"

pip install -r requirements.txt

echo "Copying code from /tmp/src to /opt/app-root"

cp -R /tmp/src /opt/app-root/

echo "changing to /opt/app-root/src"

cd /opt/app-root/src

echo "listing /opt/app-root/src"

ls -lrt /opt/app-root/src
